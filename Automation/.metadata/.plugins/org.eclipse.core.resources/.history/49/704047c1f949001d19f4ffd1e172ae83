// 121

package Twist;

public class Palendrom {

	public static void main(String[] args) {
		
		int a=141;
		int pali=0;
		int rev=0;
		while(a>0)
		{
			int rem=a%10;
			rev=(rev*0)+rem;
			a=a/10;
		}
		if(pali==rev)
		{
			System.out.println("it is palindrom");
		}
		else
			System.out.println("it is not palindrom");
	}

}
