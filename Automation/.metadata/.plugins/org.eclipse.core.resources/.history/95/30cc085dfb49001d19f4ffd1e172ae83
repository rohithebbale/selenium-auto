// reverse same

package Twist;

public class Palendrom {

	public static void main(String[] args) {
		for( int i=1; i<=1000; i++)
		{
		int a=i;
		int pali=a;
		int rev=0;
		while(a>0)
		{
			int rem=a%10;
			rev=(rev*0)+rem;
			a=a/10;
		}
		if(pali==rev)
		{
			System.out.println(pali+ "it is palindrom");
		}
		else
			System.out.println(pali+ "it is not palindrom");
	}
	}
}
