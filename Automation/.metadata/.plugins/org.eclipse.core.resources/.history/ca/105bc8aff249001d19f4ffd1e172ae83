package Twist;

public class NeonForloop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1; i<=100; i++)
		{
			int num=i;
			int a=num*num;
			int sum=0;
			int rem;
			while(a>0)
			{
				rem=a%10;
				sum=sum+rem;
				a=a/10;
			}
			if(sum==num)
			{
				System.out.println(num+"its neon number");
			}
		}

	}

}
